,"27":{"body":24,"breadcrumbs":1,"title":0},"28":{"body":15,"breadcrumbs":2,"title":1},"29":{"body":9,"breadcrumbs":1,"title":0},"3":{"body":5,"breadcrumbs":3,"title":2},"30":{"body":13,"breadcrumbs":1,"title":0},"31":{"body":8,"breadcrumbs":1,"title":0},"32":{"body":19,"breadcrumbs":1,"title":0},"4":{"body":3,"breadcrumbs":1,"title":0},"5":{"body":0,"breadcrumbs":2,"title":1},"6":{"body":4,"breadcrumbs":1,"title":0},"7":{"body":7,"breadcrumbs":1,"title":0},"8":{"body":2,"breadcrumbs":1,"title":0},"9":{"body":0,"breadcrumbs":1,"title":0}},"docs":{"0":{"body":"Rust入門 セットアップ Cargo 変数 データ型 関数 フロー制御 所有権","breadcrumbs":"Rust","id":"0","title":"Rust"},"1":{"body":"","breadcrumbs":"Rust入門 » インストール","id":"1","title":"インストール"},"10":{"body":"let a = 5;\nprintln!(\"a = {}\", a);","breadcrumbs":"Rust入門 » 変数束縛","id":"10","title":"変数束縛"},"11":{"body":"let a = 5;\nprintln!(\"a = {}\", a); a = 10;\nprintln!(\"a = {}\", a);","breadcrumbs":"Rust入門 » 変数は標準で不変である","id":"11","title":"変数は標準で不変である"},"12":{"body":"mutキーワードを使用することで、可変な変数束縛を実現できる。 let mut a = 5;\nprintln!(\"a = {}\", a); a = 10;\nprintln!(\"a = {}\", a);","breadcrumbs":"Rust入門 » 可変な変数束縛","id":"12","title":"可変な変数束縛"},"13":{"body":"不変変数とは、いくつかの違いがある。 mutキーワードを使うことはできない。 値の型を必ず定義する必要がある 定数式に使うことしかできず、関数の呼び出し結果や、実行時に評価される値に使うことはできない。 const YEAR: u32 = 2020;\nprintln!(\"YEAR = {}\", YEAR); fn main() { let a = 4; const B: u32 = a; // 実行時に評価される値 println!(\"B = {}\", B);\n}","breadcrumbs":"Rust入門 » 定数","id":"13","title":"定数"},"14":{"body":"変数を再度束縛すると、変数を 覆い隠す ことができる let a = 5;\nprintln!(\"a = {}\", a);\nlet a = 10;\nprintln!(\"a = {}\", a);\nlet a = a * a;\nprintln!(\"a = {}\", a); mutキーワードを使って、可変変数にすることと意味は異なります。 シャドーイングを行って値を加工したあと、すぐに不変変数になります。 また、値の型を変えることもできます。 let count = \"hello\"; // リテラル\nlet count = count.len(); // usize\nprintln!(\"count = {}\", count);","breadcrumbs":"Rust入門 » シャドーイング","id":"14","title":"シャドーイング"},"15":{"body":"Rustにおける値は、すべて何らかのデータ型に当てはまり、コンパイラはそれを知っている必要がある。 コンパイラは型推論を行いますが、複数の型が予測可能な場合には、型注釈をつける必要がある。","breadcrumbs":"Rust入門 » データ型","id":"15","title":"データ型"},"16":{"body":"let binary = 0b1001_1100;\nlet octal = 0o77;\nlet dechimal = 98_222;\nlet hexa = 0xff;\nlet byte = b'A'; #println!(\"binary = {}\", binary);\n#println!(\"octal = {}\", octal);\n#println!(\"decinal = {}\", dechimal);\n#println!(\"hexa = {}\", hexa);\n#println!(\"byte = {}\", byte);","breadcrumbs":"Rust入門 » 数値リテラル","id":"16","title":"数値リテラル"},"17":{"body":"型注釈をつけなった場合、i32が基準となる。 let a: i8 = -128;\nlet b: u8 = 255;\nlet c: u16 = 65_535;\nlet d: i16 = -32_768;\nlet e: u32 = 4_294_967_295;\nlet f: i32 = -2_147_483_647;\nlet g: u64 = 18_446_744_073_709_551_615;\nlet h: i64 = -9_223_372_036_854_775_808;\nlet i: usize = 1; // アーキテクチャに依存\nlet j: isize = 1; // アーキテクチャに依存 #println!(\"a = {}\", a);\n#println!(\"b = {}\", b);\n#println!(\"c = {}\", c);\n#println!(\"d = {}\", d);\n#println!(\"e = {}\", e);\n#println!(\"f = {}\", f);\n#println!(\"g = {}\", g);\n#println!(\"h = {}\", h);\n#println!(\"i = {}\", i);\n#println!(\"j = {}\", j);","breadcrumbs":"Rust入門 » 整数型","id":"17","title":"整数型"},"18":{"body":"型注釈をつけない場合、f64が基準となる。 let a: f32 = 3.0; // 単精度浮動小数点数\nlet b: f64 = 2.0; // 倍精度浮動小数点数 #println!(\"a = {}\", a);\n#println!(\"b = {}\", b);","breadcrumbs":"Rust入門 » 浮動小数点型","id":"18","title":"浮動小数点型"},"19":{"body":"型注釈をつけなくても、自動的に型推論される。 let t: bool = true;\nlet f: bool = false; #println!(\"t = {}\", t);\n#println!(\"f = {}\", f);","breadcrumbs":"Rust入門 » 論理値型","id":"19","title":"論理値型"},"2":{"body":"$ scoop install rust # Windows\n$ brew install rust # MacOS\n$ sudo apt install rustc # Ubuntu","breadcrumbs":"Rust入門 » 各OSのパッケージマネージャを使ってインストール","id":"2","title":"各OSのパッケージマネージャを使ってインストール"},"20":{"body":"let c = 'z';\nlet kanji = '漢';\nlet emoji = '⚙'; #println!(\"c = {}\", c);\n#println!(\"kanji = {}\", kanji);\n#println!(\"emoji = {}\", emoji);","breadcrumbs":"Rust入門 » 文字型","id":"20","title":"文字型"},"21":{"body":"タプルは、複数の型の複数の値を一つの複合要素としてまとめあげるものです。 タプル内の値はそれぞれ、同じ型である必要はない。 タプル名.添え字でアクセスすることができます。 let tup: (i32, f64, u8) = (500, 6.4, 1);\n#println!(\"tup = ({}, {}, {})\",tup.0, tup.1, tup.2);","breadcrumbs":"Rust入門 » タプル","id":"21","title":"タプル"},"22":{"body":"タプルとは異なり、配列の全要素は同じ型でなければならない。 Rustの配列は固定長なので、サイズを伸縮することはできません。 配列名[添え字]でアクセスすることができます。 let a = [1, 2, 3, 4, 5];\n#println!(\"a[0] = {}\", a[0]);\n#println!(\"a[1] = {}\", a[1]);\n#println!(\"a[2] = {}\", a[2]);\n#println!(\"a[3] = {}\", a[3]);\n#println!(\"a[4] = {}\", a[4]);","breadcrumbs":"Rust入門 » 配列","id":"22","title":"配列"},"23":{"body":"main関数はプログラム実行時、一番最初に走ります。 fnキーワードによって、新しい関数を宣言することができます。 Rustの慣習として、関数と変数の命名規則はスネークケースを使用します。 fn main() { println!(\"main\"); another_function();\n} fn anotehr_function() { println!(\"another function\");\n}","breadcrumbs":"Rust入門 » 関数","id":"23","title":"関数"},"24":{"body":"関数は、引数を持つように定義することができます。 引数を設定する場合、型を明示しなければなりません。 fn main() { print_xy(5, 10);\n} fn print_xy(x: i32, y: i32) { println!(\"x = {}, y = {}\",x ,y);\n}","breadcrumbs":"Rust入門 » 引数がある関数","id":"24","title":"引数がある関数"},"25":{"body":"関数が戻り値を返す場合、->を使って、戻り値の型を明示する必要があります。 関数の戻り値は、関数本体ブロック最後の「式」の値になります。 「式」には、基本的にセミコロンを付与しません。 returnキーワードは省略が可能です。 fn main() { let a = return_six(); println!(\"a = {}\",a);\n} fn return_six() -> u32 { return 6\n}","breadcrumbs":"Rust入門 » 戻り値のある関数","id":"25","title":"戻り値のある関数"},"26":{"body":"","breadcrumbs":"Rust入門 » フロー制御","id":"26","title":"フロー制御"},"27":{"body":"条件を与え、処理を分岐させることができます。 条件式は、bool型である必要があります。つまり、「0」や「1」といった条件を使うことはできません。 fn main() { let number = 6; if number % 4 == 0 { println!(\"number % 4 == 0\"); } else if number % 3 == 0 { println!(\"number % 3 == 0\"); } else if number % 2 == 0 { println!(\"number % 2 == 0\"); } else { println!(\"else\"); }\n}","breadcrumbs":"Rust入門 » if","id":"27","title":"if"},"28":{"body":"loopキーワードを使用すると、明示的に停止させない限り、無限にループを実行します。 breakキーワードを使用することで、ループを停止することができます。 fn main() { let mut count = 0; loop { if count == 5 { println!(\"finish!\"); break; } else { println!(\"again!\"); count += 1; } }\n}","breadcrumbs":"Rust入門 » loop","id":"28","title":"loop"},"29":{"body":"whileループには、条件式を設定することができます。 条件式を設定できるため、ループ回数が明確になります。 let mut count = 0;\nwhile count != 5 { println!(\"again!\"); count += 1;\n}\nprintln!(\"finish!\");","breadcrumbs":"Rust入門 » while","id":"29","title":"while"},"3":{"body":"拡張子は.rs fn main() { println!(\"Hello, World!\");\n}","breadcrumbs":"Rust入門 » Hello, World!","id":"3","title":"Hello, World!"},"30":{"body":"Rustでは最も使用頻度が高いループで、一定回数ループを繰り返したり、コレクションの中身を覗き見ることが可能。 let a = [10, 20, 30, 40, 50]; for element in a.iter() { println!(\"a = {}\", element);\n} for i in (0..5) { println!(\"a[{}] = {}\",i ,a[i]);\n}","breadcrumbs":"Rust入門 » for","id":"30","title":"for"},"31":{"body":"Rustでは、変数束縛を行うと、その変数が関連付けられた値の所有権を持つ。と表現されます。 以下の場合、スコープ内ではtomという変数が5の値を所有し、束縛していることになります。 その後、スコープを抜けるときにtomが束縛していた5の値は解放され、破棄されます。 このことを、Rustではdropと呼びます。 fn main() { let tom = 5;\n}","breadcrumbs":"Rust入門 » 所有権","id":"31","title":"所有権"},"32":{"body":"以下の例では、はじめに変数aがString型の文字列\"Hello\"を束縛しています。 次に、変数bが変数aを代入しています。 C言語やJavaなどの別の言語では、変数aの内容がbにコピーされますが、Rustでは、違う動きをします。 代入が発生したことにより、文字列\"Hello\"の所有権が変数aから変数bに移動しました。 つまり、変数aは所有権を失い、束縛するものがなくなったため、無効となります。 fn main() { let a = String::from(\"Hello\"); let b = a; println!(\"a = {}\", a); // 既にaは無効なため、コンパイルエラー println!(\"b = {}\", b);\n} ## 所有権は移動しない場合がある + 気を付けたいこととして、代入が発生したとき、必ず所有権の移動が発生するわけではありません。\n+ 上記のコードで所有権の移動が発生した原因は、`変数束縛値のサイズが変わる型を用いた`から\n+ つまり、整数のような、コンパイル時に既知のサイズを持つ型は、動作が高速なため、 ```rust\nfn main() { let a = 5; let b = a; println!(\"a = {}\", a); println!(\"b = {}\", b); }","breadcrumbs":"Rust入門 » 所有権は移る","id":"32","title":"所有権は移る"},"4":{"body":"$ rustc main.rs\n$ ./main","breadcrumbs":"Rust入門 » コンパイル ～ 実行","id":"4","title":"コンパイル ～ 実行"},"5":{"body":"","breadcrumbs":"Rust入門 » Cargo","id":"5","title":"Cargo"},"6":{"body":"$ cargo new [Project_Name] --bin","breadcrumbs":"Rust入門 » プロジェクトの作成","id":"6","title":"プロジェクトの作成"},"7":{"body":"$ cargo build\n$ cargo build --release # リリース用実行ファイルの作成\n$ cargo check # 実行用ファイルを生成しない","breadcrumbs":"Rust入門 » ビルド","id":"7","title":"ビルド"},"8":{"body":"$ cargo run","breadcrumbs":"Rust入門 » 実行","id":"8","title":"実行"},"9":{"body":"","breadcrumbs":"Rust入門 » 変数","id":"9","title":"変数"}},"length":33,"save":true},"fields":["title","body","breadcrumbs"],"index":{"body":{"root":{"0":{".":{".":{"5":{"df":1,"docs":{"30":{"tf":1.0}}},"df":0,"docs":{}},"df":0,"docs":{}},"b":{"1":{"0":{"0":{"1":{"_":{"1":{"1":{"0":{"0":{"df":1,"docs":{"16":{"tf":1.0}}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":3,"docs":{"27":{"tf":2.449489742783178},"28":{"tf":1.0},"29":{"tf":1.0}},"o":{"7":{"7":{"df":1,"docs":{"16":{"tf":1.0}}},"df":0,"docs":{}},"df":0,"docs":{}},"x":{"df":0,"docs":{},"f":{"df":0,"docs":{},"f":{"df":1,"docs":{"16":{"tf":1.0}}}}}},"1":{"0":{"df":5,"docs":{"11":{"tf":1.0},"12":{"tf":1.0},"14":{"tf":1.0},"24":{"tf":1.0},"30":{"tf":1.0}}},"2":{"8":{"df":1,"docs":{"17":{"tf":1.0}}},"df":0,"docs":{}},"8":{"_":{"4":{"4":{"6":{"_":{"7":{"4":{"4":{"_":{"0":{"7":{"3":{"_":{"7":{"0":{"9":{"_":{"5":{"5":{"1":{"_":{"6":{"1":{"5":{"df":1,"docs":{"17":{"tf":1.0}}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":0,"docs":{}},"df":5,"docs":{"17":{"tf":1.4142135623730951},"21":{"tf":1.0},"22":{"tf":1.0},"28":{"tf":1.0},"29":{"tf":1.0}}},"2":{".":{"0":{"df":1,"docs":{"18":{"tf":1.0}}},"df":0,"docs":{}},"0":{"2":{"0":{"df":1,"docs":{"13":{"tf":1.0}}},"df":0,"docs":{}},"df":1,"docs":{"30":{"tf":1.0}}},"5":{"5":{"df":1,"docs":{"17":{"tf":1.0}}},"df":0,"docs":{}},"_":{"1":{"4":{"7":{"_":{"4":{"8":{"3":{"_":{"6":{"4":{"7":{"df":1,"docs":{"17":{"tf